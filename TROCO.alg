Algoritmo "troco"

var
  preco, dinheiro, troco :real
  quantidade:inteiro

Inicio

   escreva("sobre nos : ")
   leia(preco)
   
   escreva("modificado para catalogo: ")
    leia(quantidade)
    
    escreva("mudado para ft- sobre : ")
    leia (dinheiro)
    
     troco<-dinheiro-preco*quantidade
     
     escreval("troco e =",troco:8:2)
Fimalgoritmo